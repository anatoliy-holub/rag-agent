{
  "name": "rag-agent",
  "version": "1.0.0",
  "description": "RAG agent with LM Studio and ChromaDB",
  "type": "module",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "tsx watch src/server.ts",
    "ingest": "npm run build && node dist/ingest.js",
    "ingest:dev": "tsx src/ingest.ts",
    "server": "tsx src/server.ts"
  },
  "keywords": ["rag", "lmstudio", "chromadb"],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.2",
    "chromadb": "^1.8.1",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "swagger-ui-express": "^5.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.4",
    "@types/swagger-ui-express": "^4.1.6",
    "tsx": "^4.6.2",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18"
  }
}
